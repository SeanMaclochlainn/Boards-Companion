{ 
    "manifest_version": 2, 
    "name": "BoardsUiEnhancer", 
    "version": "1.0", 
    "description": "Extension that enhances boards.ie interface", 
    
    "content_scripts": [ 
        { 
            "matches": ["*://www.boards.ie/*"], 
            "js": ["content.js" ] 
        } 
    ],

    "web_accessible_resources": [
        "main.js",
        "elementDisplayUpdater.js"
     ]
}